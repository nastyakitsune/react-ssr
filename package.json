{
  "name": "react-server-side",
  "version": "1.0.0",
  "description": "React Server Side App - done right",
  "scripts": {
    "lint:js": "eslint --ext jsx,js ./ --fix",
    "start": "rm -rf dist && webpack --mode development --config ./config/client/dev.config.js --watch",
    "server": "nodemon ./server.js",
    "build": "rm -rf build && webpack --mode production --config ./config/client/prod.config.js"
  },
  "keywords": [
    "react",
    "css-modules",
    "server-side",
    "redux",
    "react-router"
  ],
  "author": "https://github.com/nastyakitsune",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
